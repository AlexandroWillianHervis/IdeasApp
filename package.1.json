{
  "name": "chat-nest",
  "version": "0.0.0",
  "description": "poc chat cna",
  "author": "DB1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src/main.ts",
    "start:dev": "nodemon --config ./nodemon.json",
    "start:debug": "nodemon --config ./nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node index.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/microservices": "^5.4.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.2.2",
    "@nestjs/websockets": "^5.4.0",
    "@types/ioredis": "^4.0.4",
    "@types/sqs-consumer": "^3.8.0",
    "aws-sdk": "^2.362.0",
    "body-parser": "^1.18.3",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^6.2.0",
    "ioredis": "^4.2.0",
    "mime-types": "^2.1.21",
    "multer-s3": "^2.9.0",
    "nest-schedule": "^0.4.5",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^7.6.1",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "socket.io-redis": "^5.2.0",
    "sqs-consumer": "^3.8.0",
    "typeorm": "^0.2.9",
    "typescript": "^3.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "lerna": "^3.4.3",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
